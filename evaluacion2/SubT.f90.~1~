    !iniciamos subrutina
   subroutine expT10(n,x)
     double precision, intent(in)::x
     integer,intent(in)::n
     double precision, intent(out):: expT10
     double precision:: term, partial_sum
     doible precision, parameter:: x(0,10)
     integer::j
     integer,parameter:: ntimes=100

     term=1
     partial_sum=term

     do j=1,ntimes
        term=term*(x/j)
        partial_sum=partial_sum+term
        end do




  
