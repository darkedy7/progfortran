
subroutine Ap(x,n,y)
  implicit none
  integer(kind=8) ,dimension(100),  intent (in)::n
  real (kind=8),dimension(100), intent (in) ::x
  real (kind=8),dimension(100),  intent (out)::y
  !variables locales
  real(kind=8)::term, partial_sum
  integer::i
 term=1
 partial_sum::term
 
 do i=1,n,2
    !calcular el factorial
    term=term*x/i
    !agrega este t√©rmino a la suma parcial
    partial_sum=partial_term + term
    end do
    y(i)=partial_sum    
    
     
     
program Taylor
  implicit none

end program Taylor
  


