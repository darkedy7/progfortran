subroutine expT(x,n,expt)
  real(kind=8),dimension(100),  intent(in)::x
  real(kind=8), intent(out)::expt
  !variables
  real(kind=8):: term,partial_sum
  integer,parameter:: ntimes=15
  integer::j
  term=1
  partial_sum=term
  do j=1,ntimes
    f(j)=float(j)
     term=term*(x/j*float(j) )


  !
